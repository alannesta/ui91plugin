{
  "name": "91lister",
  "version": "2.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "downloads",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
   "browser_action": {
       "default_title": "Video",
       "default_popup": "popup.html"
   },

  "manifest_version": 2
}
