{
  "name": "91lister",
  "version": "2.0",
  "permissions": [
    "tabs",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.sinoquebec.com/bbs/*"
      ],
      "js": [
        "contentjs.js"
      ]
    }
  ],
   "browser_action": {
       "default_title": "Video",
       "default_popup": "popup.html"
   },

  "manifest_version": 2
}
